{
  "name": "waok-schedule",
  "version": "1.0.0",
  "description": "Sistema de gestiÃ³n de horarios para WAOK",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:win": "nodemon server.js",
    "test": "jest",
    "test:accessibility": "node scripts/accessibility-test.js",
    "test:a11y": "node scripts/accessibility-test.js",
    "test:cross-browser": "playwright test --config=playwright.cross-browser.config.ts",
    "test:browserstack": "BROWSERSTACK_USERNAME=$BROWSERSTACK_USERNAME BROWSERSTACK_ACCESS_KEY=$BROWSERSTACK_ACCESS_KEY playwright test --config=browserstack.config.ts",
    "test:saucelabs": "SAUCE_USERNAME=$SAUCE_USERNAME SAUCE_ACCESS_KEY=$SAUCE_ACCESS_KEY playwright test --config=saucelabs.config.ts",
    "test:chrome": "playwright test --config=playwright.cross-browser.config.ts --project='Desktop Chrome'",
    "test:firefox": "playwright test --config=playwright.cross-browser.config.ts --project='Desktop Firefox'",
    "test:safari": "playwright test --config=playwright.cross-browser.config.ts --project='Desktop Safari'",
    "test:edge": "playwright test --config=playwright.cross-browser.config.ts --project='Desktop Edge'",
    "test:mobile": "playwright test --config=playwright.cross-browser.config.ts --project='Mobile Chrome' --project='Mobile Safari'",
    "lint": "eslint .",
    "build": "webpack --mode production"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "axe-core": "^4.8.2",
    "pa11y": "^6.2.3",
    "lighthouse": "^11.0.0",
    "chrome-launcher": "^1.1.0",
    "puppeteer": "^21.3.6"
  },
  "keywords": [
    "schedule",
    "management",
    "waok",
    "accessibility",
    "wcag"
  ],
  "author": "ARCHITECT-AI",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
